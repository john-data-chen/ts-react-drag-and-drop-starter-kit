{
  "arrowParens": "always",
  "bracketSpacing": true,
  "semi": false,
  "useTabs": false,
  "trailingComma": "none",
  "singleQuote": true,
  "tabWidth": 2,
  "endOfLine": "lf",
  "printWidth": 80,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}